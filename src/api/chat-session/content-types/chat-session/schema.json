{
  "kind": "collectionType",
  "collectionName": "chat_sessions",
  "info": {
    "singularName": "chat-session",
    "pluralName": "chat-sessions",
    "displayName": "ChatSession"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "uid": {
      "type": "string",
      "unique": true
    },
    "sessionid": {
      "type": "string",
      "unique": true
    },
    "starttime": {
      "type": "datetime"
    },
    "endtime": {
      "type": "datetime"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "chatsession"
    }
  }
}



